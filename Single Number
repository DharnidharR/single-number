#include<stdio.h>
int main()
{
    int n,result=0,i;
    printf("Array Size : ");
    scanf("%d",&n);
    printf("Enter %d Array Elements : ",n);
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        result=result^a[i];
    }
    printf("Unique Number is : %d",result);
}
